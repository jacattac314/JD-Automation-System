# API Keys (DO NOT COMMIT THE ACTUAL .env FILE)

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# GitHub
GITHUB_TOKEN=ghp_your_github_personal_access_token_here
GITHUB_USERNAME=your_github_username

# Anthropic (for Claude Code)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LinkedIn (optional)
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret
LINKEDIN_ACCESS_TOKEN=your_linkedin_oauth_token

# Configuration
DEFAULT_REPO_VISIBILITY=private
PROJECT_STORAGE_PATH=./projects
ENABLE_LINKEDIN_INTEGRATION=false
LOG_LEVEL=INFO

# Antigravity/Claude Code settings
CLAUDE_CODE_PATH=claude
ANTIGRAVITY_ENABLED=true
CODE_EXECUTION_TIMEOUT=600
